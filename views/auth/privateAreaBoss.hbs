<div class="container row justify-content-between">
    <h2>Users</h2>
    <a href="/addMember" type="submit" class="btn btn-dark">Add Member</a>
</div>

<div class="container">
    <div class="row">
        {{#each allUsers}}
            <div class="col-sm-4 memberContainer">
                {{> memberPartial this}}
                <div class="bossButtonsContainer">
                    <a href="/editMember/{{id}}" type="submit" class="btn btn-warning">Edit</a>
                    <form action="/deleteMember/{{id}}" method="POST">
                        <button type="submit" class="btn btn-danger">Delete</button>
                    </form>
                </div>
            </div>
        {{/each}}
    </div>
</div>



